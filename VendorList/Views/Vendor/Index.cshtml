@{
  Layout = "_Layout";
}

@using List.Models;

    <h3>Vendors:</h3>

    @if (@Model.Count == 0)
    {
      <h3>No Vendors have been added yet</h3>
    } 

    @foreach (Vendor vendor in Model)
    {
      <h5><a href='/vendor/@vendor.Id'>@vendor.Name</a></h5>
    }
    <p><a href='/vendor/new'>Add a new Vendor</a></p>
    <p><a href='/'>Back home</a></p>